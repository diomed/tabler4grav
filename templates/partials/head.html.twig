<meta charset="utf-8"/>
<title>
  {% if header.title %}
    {{ header.title|e('html') }}
    |
  {% endif %}{{ site.title|e('html') }}</title>

<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no">
{% include 'partials/metadata.html.twig' %}

    {% if config.theme.favicon %}
    <link rel="shortcut icon" href="{{ theme_url }}/images/{{ config.theme.favicon}}">
    {% endif %}<link rel="canonical" href="{{ page.url(true, true) }}"/>
    <style>
      @import url('https://rsms.me/inter/inter.css');
      :root {
        --tblr-font-sans-serif: 'Inter Var', -apple-system, BlinkMacSystemFont, San Francisco, Segoe UI, Roboto, Helvetica Neue, sans-serif;
      }
      body {
        font-feature-settings: "cv03", "cv04", "cv11";
      }
    </style>
    {% block stylesheets %}
        {% do assets.addCss('theme://dist/css/tabler.min.css', 98) %}
                {% do assets.addCss('theme://dist/css/demo.min.css', 98) %}

    {% endblock %}

    {% block javascripts %}
        {% do assets.addJs('theme://dist/js/tabler.min.js', {priority: 102}) %}
        {% do assets.addJs('theme://dist/js/app.js', 101) %}
        {% do assets.addJs('theme://dist/js/demo.min.js', {loading: 'defer', priority: 100}) %}
    {% endblock %}

      {% block assets deferred %}
        {{ assets.css()|raw }}
        {{ assets.js()|raw }}
    {% endblock %}